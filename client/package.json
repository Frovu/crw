{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "homepage": "/w",
  "type": "module",
  "dependencies": {
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.34",
    "@types/react-dom": "^18.0.11",
    "@uiw/react-color": "^2.0.3",
    "@vitejs/plugin-react": "^4.2.1",
    "immer": "^10.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.12",
    "react-query": "^3.39.3",
    "regression": "^2.0.1",
    "typescript": "^5.0.4",
    "uplot": "^1.6.24",
    "uplot-react": "^1.1.4",
    "vite": "^5.0.10"
  },
  "scripts": {
    "start": "vite --port 3000",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "rules": {
      "space-in-parens": [
        0,
        "always"
      ],
      "object-curly-spacing": [
        1,
        "always"
      ],
      "no-multiple-empty-lines": [
        1,
        {
          "max": 1,
          "maxEOF": 0,
          "maxBOF": 0
        }
      ],
      "quotes": [
        1,
        "single",
        "avoid-escape"
      ],
      "no-use-before-define": "off",
      "@typescript-eslint/no-use-before-define": [
        "error"
      ],
      "@typescript-eslint/semi": [
        1,
        "always"
      ],
      "semi": [
        1,
        "always"
      ],
      "prefer-const": 1,
      "react/prefer-es6-class": 0,
      "react/jsx-filename-extension": 0,
      "react/no-array-index-key": [
        1
      ],
      "no-undef": [
        2
      ],
      "no-case-declarations": [
        1
      ],
      "no-return-assign": [
        1
      ],
      "no-param-reassign": [
        1
      ],
      "no-shadow": [
        1
      ],
      "no-underscore-dangle": [
        0,
        "always"
      ],
      "indent": "off",
      "@typescript-eslint/indent": [
        1,
        "tab"
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/regression": "^2.0.2",
    "eslint-config-react-app": "^7.0.1",
    "zustand": "^4.4.1"
  }
}
