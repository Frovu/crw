{
	"forbush_effects": {
		"_constraint": "UNIQUE(time)",
		"time": {
			"not_null":true,
			"type": "time",
			"description": "Event onset time",
			"parseName": "Time"
		},
		"onset_type": {
			"name": "onset type",
			"type": "enum",
			"enum": ["SSC", "iSW", "SI"],
			"description": "Onset time origin: SSC; SI - Sudden impulse, ground data; iSW - satellite data",
			"parseName": "OType",
			"parseValue": {
				"1": "SSC",
				"2": "iSW",
				"3": "SI",
				"9": null
			}
		},
		"magnitude": {
			"name": "magnitude",
			"description": "Range between min and max CR density variation correcetd for Dst, corrected for trend, in window from ons-2h to next event or ons+72h",
			"generic": {
				"type": "range",
				"series": "a10m"
			}
		},
		"v_before": {
			"name": "V before",
			"generic": {
				"type": "value",
				"series": "v_sw",
				"poi": "forbush_effects",
				"shift": -1
			}
		},
		"v_max": {
			"generic": {
				"type": "max",
				"series": "v_sw"
			}
		},
		"b_before": {
			"name": "B before",
			"generic": {
				"type": "value",
				"series": "imf",
				"poi": "forbush_effects",
				"shift": -1
			}
		},
		"b_max": {
			"generic": {
				"type": "max",
				"series": "imf"
			}
		},
		"bz_min": {
			"generic": {
				"type": "min",
				"series": "bz"
			}
		},
		"kp_max": {
			"generic": {
				"type": "max",
				"series": "kp"
			}
		},
		"vmbm": {
			"computed": true,
			"name": "VmBm",
			"description": "Vmax / 400 * Bmax / 5"
		},
		"ap_max": {
			"generic": {
				"type": "max",
				"series": "ap"
			}
		},
		"dst_min": {
			"generic": {
				"type": "min",
				"series": "dst"
			}
		},
		"axy_max": {
			"generic": {
				"type": "max",
				"series": "axy"
			}
		},
		"az_range": {
			"generic": {
				"type": "range",
				"series": "az"
			}
		},
		"old_magnitude": {
			"name": "_magnitude",
			"description": "FD magnitude as maximum 10 GV CR density variation obtained using GSM corrected for magnetospheric effect using the Dst-index",
			"parseName": "MagnM"
		},
		"gamma": {
			"name": "gamma",
			"description": "rigidity spectrum exponent during the hour of minimum CR density",
			"parseName": "GammaM"
		},
		"comment": {
			"type": "text",
			"description": "Additional information"
		},
		"source_id": {
			"references": "solar_sources"
		},
		"mc_id": {
			"references": "magnetic_clouds"
		}
	},
	"solar_sources": {
		"time": {
			"type": "time",
			"parseName": "STime"
		},
		"type": {
			"type": "real",
			"range": [1, 99],
			"description": "It's complicated",
			"parseName": "SType"
		},
		"description": {
			"name": "info",
			"type": "text",
			"parseName": "Source",
			"parseStub": "_None____"
		},
		"confidence": {
			"type": "enum",
			"enum": ["low", "okay", "high"],
			"parseName": "Qs",
			"parseValue": {
				"3": "low",
				"4": "okay",
				"5": "high"
			}
		},
		"cme_id": {
			"references": "coronal_mass_ejections"
		},
		"ch_id": {
			"references": "coronal_holes"
		},
		"sf_id": {
			"references": "solar_flares"
		}
	},
	"coronal_mass_ejections": {
		"time": {
			"type": "time",
			"parseName": "CMETime"
		},
		"v_start": {
			"description": "CME velocity near the Sun",
			"parseName": "Vsp"
		},
		"v_mean_0": {
			"description": "CME initial velocity from coronagraph",
			"parseName": "VMean0"
		},
		"v_mean_x": {
			"description": "CME average velocity using X-ray flare time",
			"parseName": "Vmean"
		},
		"v_mean_c": {
			"description": "CME average velocity using CME observation time",
			"parseName": "VmeanC"
		},
		"width": {
			"description": "Angular width",
			"parseName": "CMEWidth"
		},
		"angle": {
			"description": "Direction of the maximum velocity",
			"parseName": "CMEAngle"
		}
	},
	"coronal_holes": {
		"polarity": {
			"parseName": "SPol"
		}
	},
	"solar_flares": {
		"description": {
			"name": "info",
			"type": "text",
			"parseName": "Fdata",
			"parseStub": "None"
		},
		"magnitude": {
			"parseName": "Xmagn",
			"description": "Maximum X-ray power (W/m/m)"
		}
	},
	"magnetic_clouds": {
		"time": {
			"not_null": true,
			"type": "time",
			"parseName": "MCStartTime"
		},
		"originator": {
			"type": "text"
		},
		"duration": {
			"parseName": "MCDur"
		},
		"size": {
			"parseName": "RMC"
		},
		"_constraint": "UNIQUE (time)"
	}
}