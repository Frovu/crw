{
	"forbush_effects": {
		"_constraint": "UNIQUE(time)",
		"time": {
			"not_null": true,
			"data_type": "time",
			"description": "Event onset time",
			"parse_name": "Time"
		},
		"duration": {
			"data_type": "integer",
			"description": "Effective event duration, hours",
			"parse_name": "Te"
		},
		"onset_type": {
			"pretty_name": "ons type",
			"data_type": "enum",
			"enum": ["SSC", "iSW", "SI"],
			"description": "Onset time origin: SSC; SI - Sudden impulse, ground data; iSW - satellite data",
			"parse_name": "OType",
			"parse_value": {
				"1": "SSC",
				"2": "iSW",
				"3": "SI",
				"9": null
			}
		},
		"s_type": {
			"pretty_name": "src type",
			"data_type": "real",
			"description": "Solar source index (it's complicated)",
			"parse_name": "SType"
		},
		"s_description": {
			"pretty_name": "src info",
			"data_type": "text",
			"description": "Solar source description",
			"parse_name": "Source"
		},
		"s_confidence": {
			"pretty_name": "src conf",
			"data_type": "enum",
			"enum": ["low", "okay", "high"],
			"description": "Solar source confidence",
			"parse_name": "Qs",
			"parse_value": {
				"3": "low",
				"4": "okay",
				"5": "high"
			}
		},
		"old_magnitude": {
			"pretty_name": "_magnitude",
			"description": "(AB) FD magnitude as maximum 10 GV CR density variation obtained using GSM corrected for magnetospheric effect using the Dst-index",
			"parse_name": "MagnM"
		},
		"gamma": {
			"pretty_name": "gamma",
			"description": "(AB) rigidity spectrum exponent during the hour of minimum CR density",
			"parse_name": "GammaM"
		},
		"vmbm": {
			"computed": true,
			"pretty_name": "VmBm",
			"description": "Vmax / 400 * Bmax / 5"
		},
		"comment": {
			"data_type": "text",
			"description": "Additional information"
		}
	},
	"magnetic_clouds": {
		"fe_id": {
			"references": "forbush_effects"
		},
		"time": {
			"not_null": true,
			"data_type": "time",
			"parse_name": "MCStartTime"
		},
		"originator": {
			"data_type": "text"
		},
		"duration": {
			"parse_name": "MCDur"
		},
		"size": {
			"parse_name": "RMC"
		}
	},
	"coronal_mass_ejections": {
		"fe_id": {
			"references": "forbush_effects"
		},
		"time": {
			"data_type": "time",
			"parse_name": "CMETime"
		},
		"v_start": {
			"description": "CME velocity near the Sun",
			"parse_name": "Vsp"
		},
		"v_mean_0": {
			"description": "CME initial velocity from coronagraph",
			"parse_name": "VMean0"
		},
		"v_mean_x": {
			"description": "CME average velocity using X-ray flare time",
			"parse_name": "Vmean"
		},
		"v_mean_c": {
			"description": "CME average velocity using CME observation time",
			"parse_name": "VmeanC"
		},
		"width": {
			"description": "Angular width",
			"parse_name": "CMEWidth"
		},
		"angle": {
			"description": "Direction of the maximum velocity",
			"parse_name": "CMEAngle"
		}
	},
	"coronal_holes": {
		"fe_id": {
			"references": "forbush_effects"
		},
		"polarity": {
			"parse_name": "SPol"
		}
	},
	"solar_flares": {
		"fe_id": {
			"references": "forbush_effects"
		},
		"time": {
			"data_type": "time",
			"parse_name": "STime",
			"description": "STime"
		},
		"description": {
			"pretty_name": "info",
			"data_type": "text",
			"parse_name": "Fdata"
		},
		"magnitude": {
			"parse_name": "Xmagn",
			"description": "Maximum X-ray power (W/m/m)"
		}
	}
}